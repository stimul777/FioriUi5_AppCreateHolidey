{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "sap.myApps",
		"type": "application",
		"i18n": "i18n/i18n.properties"
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"favIcon": "Icon/favicon.ico"
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": false
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"rootView": {
	          "viewName": "sap.myApps.zcreate_request.zcreate_request",
	          "type": "XML",
	          "async": true,
	          "id": "create_request"
	    },
		"_version": "1.1.0",
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"dependencies": {
			"minUI5Version": "1.38.1",
			"libs": {
				"sap.m": {
					
				},
				"sap.me": {
					
				},
				"sap.viz": {
					
				},
				"sap.suite.ui.microchart": {
					
				},
				"sap.ca.scfld.md": {
					
				},
				"sap.ui.layout": {
					
				},
				"sap.ui.table": {
				
				},
				"sap.ui.commons": {
				
				}
			}
		}
	}
}